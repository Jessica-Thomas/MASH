@addTagHelper*, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, AuthoringTagHelpers


@model MASH.Models.DestinyModel;

<html>
<head>
    <title>M.A.S.H.</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Special+Elite&display=swap" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css2?family=Shadows+Into+Light&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="css/styles.css" />
</head>

<body>
    <header>
        <h1 class="">M.A.S.H.</h1>
        <h2>Your Results Are In...</h2>
    </header>

    <main class="body">

        <table>
            <tr>
                <td>You will live in a:  </td>
                <td>   @Model.RandHouse</td>
            </tr>

            <tr>
                <td>You will marry:  </td>
                <td>   @Model.RandSpouse</td>
            </tr>

            <tr>
                <td>Your transportation is:  </td>
                <td>   @Model.RandTranspo</td>
            </tr>

            <tr>
                <td>You will be a(n): </td>
                <td>   @Model.RandJob</td>
            </tr>
            <tr>
                <td>You will have a pet:  </td>
                <td>   @Model.RandPet</td>
            </tr>

        </table>

    </main>
</body>
</html>
